<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="Freetuga.Repositorio" name="Freetuga repositorio" version="0.0.5" provider-name="Freetuga">
	<extension point="xbmc.addon.repository" name="Boxcenter Add-on Repository">
		<info compressed="false">http://raw.github.com/boxcenterkodi/Freetuga.Repositorio/master/addons.xml</info>
		<checksum>http://raw.github.com/boxcenterkodi/Freetuga.Repositorio/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/boxcenterkodi/Freetuga.Repositorio/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Freetuga Addon</summary>
		<description>Addon Description</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.video.freetuga" name="FreeTuga" version="0.1.1" provider-name="Korea, Vitorgalo, AndryC, Paulo Santos">
	<requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.beautifulsoup" version="3.2.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">assista tv ao vivo</summary>
    <description lang="en">nao transmitimos nenhum stream,tudo o que esta aqui esta ao alcance de qualquer um na internet!</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
